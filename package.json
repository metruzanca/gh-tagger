  "bugs": {
    "url": "https://github.com/metruzanca/gh-tagger/issues"
  },
  "homepage": "https://github.com/metruzanca/gh-tagger#readme",
  "devDependencies": {
    "@types/inquirer": "^7.3.3",
    "@types/node": "^16.6.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "exec-sh": "^0.4.0",
    "inquirer": "^8.1.2"
  },
  "semverPaths": [
    "./package.json"
